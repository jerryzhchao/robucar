<robot name="robucar_robot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:arg name="laser_enabled" default="false" />
  <xacro:arg name="camera_enabled" default="false" />
  <xacro:arg name="navsat_enabled" default="true" />
  <xacro:arg name="velodyne_enabled" default="true" />

  <xacro:include filename="$(find robucar_description)/urdf/robucar.urdf.xacro" />
  <xacro:robucar />

</robot>
