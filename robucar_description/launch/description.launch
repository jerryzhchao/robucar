<launch>

  <!--<arg name="model" default="_4ws"/>-->
  <arg name="model" default=""/>
  <arg name="laser_enabled" default="true" />
  <arg name="camera_enabled" default="false" />

  <param name="robot_description" command="$(find xacro)/xacro.py --inorder $(find robucar_description)/urdf/description$(arg model).xacro
    laser_enabled:=$(arg laser_enabled)
    camera_enabled:=$(arg camera_enabled)" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

</launch>
